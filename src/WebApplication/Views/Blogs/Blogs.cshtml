@model IEnumerable<WebApplication.Models.Blog >
@{
    ViewBag.Title = "Blogs";
}
<h2>Blogs</h2>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Url</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BlogId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Url)
            </td>
        </tr>
    }
</table>
